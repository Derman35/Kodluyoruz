   "source": [
    "# Kullanıcıdan bir sayı al\n",
    "sayi = int(input(\"Bir sayı girin: \"))\n",
    "\n",
    "# Sayının basamaklarını topla\n",
    "toplam = 0\n",
    "while sayi > 0:\n",
    "    basamak = sayi % 10\n",
    "    toplam += basamak\n",
    "    sayi //= 10\n",
    "\n",
    "# Sonucu ekrana yazdır\n",
    "print(\"Sayının basamaklarının toplamı:\", toplam)\n"
